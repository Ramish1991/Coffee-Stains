<p id="notice"><%= notice %></p>


<div class="banner clearfix">

  <button>Background</button>
  <button>Writings</button>
  <button>Contributions</button>

</div>


<p>
  <b>Cover Page:</b>
  <%= link_to image_tag(@user.profile_cover_page.thumb.url), @user.profile_cover_page.url %>
</p>

<p>
  <b>Profile Photo:</b>
  <%= link_to image_tag(@user.profile_photo.thumb.url), @user.profile_photo.url %>
</p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<!-- <p>
  <strong>Currently Working On:</strong>
  <%#= @user.currently_working_on %>
</p> -->

<p>
  <strong>Mood:</strong>
  <%= @user.mood %>
</p>

<p>
  <strong>Quote:</strong>
  <%= @user.Quote %>
</p>

<div class="Snippet-container">
  <p >
    <strong>Snippet title:</strong>
    <%= @user.snippet_title %>
    <br>
    <br>
    <strong>Snippet content:</strong>
    <%= @user.snippet_content %>
  </p>
</div>
